{
  "extends": [
    "config:base"
  ],
  "reviewers": [
    "codecov/open-source"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["pkg"],
      "allowedVersions": "4.4.8"
    },
    {
      "matchDepTypes": ["devDependencies"],
      "matchUpdateTypes": ["patch", "minor"],
      "groupName": "devDependencies (non-major)"
    }
    ]
}
