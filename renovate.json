{
  "extends": [
    "config:base",
    "docker:pinDigests",
    "npm:unpublishSafe",
    "helpers:pinGitHubActionDigests"
  ],
  "reviewers": [
    "codecov/open-source"
  ],
  "packageRules": [
    {
      "matchDepTypes": ["devDependencies"],
      "matchUpdateTypes": ["patch", "minor"],
      "groupName": "devDependencies (non-major)"
    }
  ],
  "circleci" : {
    "pinDigests": true
  }
}
