{
  "name": "codecov",
  "version": "0.1.0",
  "description": "Codecov Report Uploader",
  "private": true,
  "bin": "bin/codecov",
  "scripts": {
    "test": "nyc mocha test/*.test.js test/**/*.test.js",
    "prebuild": "rollup -c rollup.config.js",
    "build": "pkg dist/codecov.js --out-path out"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codecov/uploader.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/codecov/uploader/issues"
  },
  "homepage": "https://github.com/codecov/uploader#readme",
  "dependencies": {
    "chalk": "^3.0.0",
    "figlet": "^1.2.4",
    "superagent": "^5.2.1",
    "validator": "^12.1.0",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "chai": "^4.2.0",
    "eslint": "^6.7.2",
    "mocha": "^7.0.0",
    "nyc": "^15.0.0",
    "pkg": "^4.4.1",
    "rollup": "^1.31.0"
  },
  "nyc": {
    "all": true,
    "reporter": [
      "text",
      "cobertura"
    ]
  }
}
