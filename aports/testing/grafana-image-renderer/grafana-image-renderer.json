{
  "service": {
    "host": "127.0.0.1",
    "port": 3001,
    "metrics": {
      "enabled": false,
      "collectDefaultMetrics": true,
      "requestDurationBuckets": [1, 5, 7, 9, 11, 13, 15, 20, 30]
    },
    "logging": {
      "level": "info",
      "console": {
        "json": true,
        "colorize": false
      }
    }
  },
  "rendering": {
    "timezone": null,
    "chromeBin": "/usr/bin/chromium-browser",
    "ignoresHttpsErrors": false,
    "timingMetrics": false,
    "mode": "default",
    "clustering": {
      "mode": "browser",
      "maxConcurrency": 5
    },
    "verboseLogging": false
  }
}
